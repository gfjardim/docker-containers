<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>H5AI</Name>
  <Description>
    H5AI is a modern web server index. This docker image makes it trivially easy to spin up a webserver and start sharing your files through the web.[br][br]
    [b][span style='color: #E80000;']Directions:[/span][/b][br]
    [b]/config[/b] : this path is used to store the configuration H5AI.[br]
    [b]/var/www[/b] : this path is that will be shared via a web index by H5AI.
  </Description>
  <Registry>https://registry.hub.docker.com/u/smdion/docker-h5ai/</Registry>
  <Repository>smdion/docker-h5ai</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8888</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name></Name>
      <Value></Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/h5ai</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/var/www</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:80]/</WebUI>
  <Banner></Banner>
  <Icon>http://i.imgur.com/uN4z6eL.png</Icon>
</Container>